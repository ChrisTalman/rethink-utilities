{
    "compilerOptions":
    {
        "target": "es2015",
        "module": "ES6",
        "moduleResolution": "node",
        "baseUrl": "./",
		"lib":
		[
			"es2015",
			"es2017"
		],
        "removeComments": true,
        "downlevelIteration": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": true,
		"paths":
		{
			"src": ["./src"]
		}
    }
}